{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "types": ["jest", "@testing-library/jest-dom"],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "global-types.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "declarations.d.ts", // добавьте ваш файл определений типов
    ".next/types/**/*.ts",
    "src/types/*.d.ts"
, "src/pages/privetofficepage.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
